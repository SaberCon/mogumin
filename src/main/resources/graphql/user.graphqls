extend type Query {
 getUserById(id: ID): User
 getAllUsers: [User!]!
}

extend type Mutation {
 addUser(UserInput: UserInput!): User
 updateUser(id: ID, UserInput: UserInput!): User
 deleteUserById(id: ID): User
}

type User {
 id: ID!
 username: String!
 password: String!
 phone: String!
 avatar: String!
 ctime: String!
 mtime: String!
}

input UserInput {
 username: String!
 password: String!
 phone: String!
 avatar: String!
}